{
    "compilerOptions": {
        "allowJs": true,
        "experimentalDecorators": true,
        "jsx": "preserve",
        "lib": [
            "es7",
            "dom"
        ],
        "module": "es2015",
        "moduleResolution": "node",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "preserveSymlinks": true,
        "pretty": true,
        "strict": true,
        "target": "es5"
    }
}